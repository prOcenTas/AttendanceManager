<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidattendance">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidAttendance">
        <activity
            android:name=".UserActivitys.TeacherActivitys.UserActivity"
            android:parentActivityName=".StartActivitys.LoginActivity"
            android:theme="@style/Theme.AndroidAttendance.NoActionBar" />
        <activity
            android:name=".UserActivitys.AdminActivitys.AdminActivity"
            android:parentActivityName=".StartActivitys.LoginActivity"
            android:theme="@style/Theme.AndroidAttendance.NoActionBar" />
        <activity android:name=".UserActivitys.AdminActivitys.AddingStudent.AdminAddStudent"

            android:theme="@style/Theme.AndroidAttendance.NoActionBar" />
        <activity
            android:name=".StartActivitys.LoginActivity"
            android:parentActivityName=".StartActivitys.MainActivity"
            android:theme="@style/Theme.AndroidAttendance.NoActionBar" />
        <activity
            android:name=".UserActivitys.AdminActivitys.RegisterUser"
            android:parentActivityName=".UserActivitys.AdminActivitys.AdminActivity"
            android:theme="@style/Theme.AndroidAttendance.NoActionBar" />
        <activity
            android:name=".StartActivitys.MainActivity"
            android:parentActivityName=".StartActivitys.MainActivity"
            android:theme="@style/Theme.AndroidAttendance.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>